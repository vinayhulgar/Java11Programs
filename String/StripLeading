public class Repeat {
    public static void main(String args[]) {
      String str = "    OnlineJavaIde";
      System.out.println(str.stripLeading());
      
       String str = "    OnlineJavaIde   ";
      System.out.println(str.strip());
      
      String str = "OnlineJavaIde   ";
      System.out.println(str.stripTrailing());
      
    }
}
